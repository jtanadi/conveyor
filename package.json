{
  "name": "pdf2img",
  "version": "0.5.3",
  "description": "A microservice for converting PDFs to images and uploading them to AWS S3.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev:local": "LOCAL=true npm run dev",
    "start": "node --optimize_for_size --max_old_space_size=460 dist/index.js",
    "dev:test-server": "ts-node test-server/index.ts & npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "^12.6.1"
  },
  "author": "JT",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.654.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "nanoid": "^3.0.2",
    "pdf2json": "raa-tools/pdf2json#master",
    "restana": "^4.3.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.11.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
